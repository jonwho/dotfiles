{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "settings": {
    "react": {
      "pragma": "React"
    },
    "ecmascript": 6,
    "jsx": true
  },
  "rules": {
    "quotes": 0,
    "strict": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "React": true,
    "Angular": true,
    "angular": true,
    "console": true,
    "$": true
  }
}
